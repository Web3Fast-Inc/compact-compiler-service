# Compact Compiler Service Configuration

# Environment
NODE_ENV=production

# Server Configuration
PORT=3002
HOST=0.0.0.0

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/compact-service.log

# Compilation Settings
COMPACTC_PATH=/usr/local/bin/compactc
ZKIR_PATH=/usr/local/bin/zkir
TEMP_DIR=./temp
MAX_COMPILE_TIME=300000
CLEANUP_TEMP_FILES=true

# Security
MAX_REQUEST_SIZE=10mb
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Health Check
HEALTH_CHECK_INTERVAL=30000

# Service URL (for Web3Fast integration)
COMPACT_SERVICE_URL=http://localhost:3002 